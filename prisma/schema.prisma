generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  Id       Int    @id @default(autoincrement())
  Username String @unique
  Password String
}

model Videos {
  Id   Int    @id @default(autoincrement())
  Path String @unique
}

model validation {
  id       Int    @id @default(autoincrement())
  password String
}
